// Secrets.xcconfig.example
// Copy this file to Secrets.xcconfig and fill in your values.
// DO NOT commit Secrets.xcconfig to source control.

// Supabase auth (login / signup / token refresh only)
SUPABASE_URL = https:/$()/your-project.supabase.co
SUPABASE_ANON_KEY = your-anon-key-here

// Fallback used only for auth routes — keep pointing at the web app or Supabase
API_BASE_URL = https:/$()/your-web-app.vercel.app

// AWS API Gateway — ALL data endpoints route here:
//   profile, inputs, jobs, recommendations, products, pricing,
//   upload, feedback, retrieval/search
// Set this to your API Gateway invoke URL (no trailing slash).
// Format: https://<api-id>.execute-api.<region>.amazonaws.com/<stage>
API_RUNTIME_BASE_URL = https:/$()/your-api-id.execute-api.us-west-2.amazonaws.com
