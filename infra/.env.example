# Required when CDK cannot infer account from your local AWS profile/session.
AWS_ACCOUNT_ID=325460142505

# Target region for infrastructure deployment.
AWS_REGION=ca-west-1

# Environment selector: dev | staging | prod
CROP_ENV=dev

# API persistence backend
DATA_BACKEND=postgres
DATABASE_URL=

# Transitional auth verifier (use Cognito OR Supabase verifier settings)
COGNITO_REGION=
COGNITO_USER_POOL_ID=
COGNITO_APP_CLIENT_ID=
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Optional monthly budget override in USD.
MONTHLY_BUDGET_USD=150

# Optional average recommendation cost alert threshold in USD.
MAX_RECOMMENDATION_COST_USD=1.50

# Optional recommendation cost telemetry overrides.
RECOMMENDATION_COST_USD=0.81
RECOMMENDATION_COST_BY_MODEL_JSON={}

# Optional CloudWatch namespace used by API EMF recommendation metrics.
METRICS_NAMESPACE=CropCopilot/Pipeline

# Optional cost alert destination for Budget notifications.
COST_ALERT_EMAIL=
